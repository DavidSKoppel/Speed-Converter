<Window x:Class="Speed_Converter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Speed_Converter"
        mc:Ignorable="d"
        Title="MainWindow" Height="368" Width="306">
    <Grid>
        <Label x:Name="Label1" Content="Calculations" HorizontalAlignment="Left" Margin="22,75,0,0" VerticalAlignment="Top" Width="251"/>
        <Button x:Name="Button1" Content="Click to change between m/s and MPH" HorizontalAlignment="Left" Margin="22,151,0,0" VerticalAlignment="Top" Width="251" Click="Button1_Click" Height="57.96"/>
        <TextBox x:Name="TextBox1" HorizontalAlignment="Left" Height="23" Margin="22,47,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120" TextChanged="TextBox1_TextChanged"/>
        <TextBlock HorizontalAlignment="Left" Height="34" Margin="22,223,0,0" TextWrapping="Wrap" Text="Calculations changes after you type in new data" VerticalAlignment="Top" Width="163"/>
    </Grid>
</Window>
